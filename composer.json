{
    "name": "benr77/fetch-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for fetching mails from IMAP",
    "keywords": ["mail","imap"],
    "homepage": "http://github.com/benr77/fetch-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Karisch",
            "email": "patrik.karisch@abimus.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.1|~3.0|~4.0",
        "tedivm/fetch": "~0.4.3"
    },
    "autoload": {
        "psr-0": { "Abimus\\FetchBundle": "" }
    },
    "target-dir": "Abimus/FetchBundle",
	"extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
